{
  "name": "pressgang-wp/installer",
  "description": "PressGang theme installer.",
  "keywords": ["pressgang", "wordpress"],
  "license": "MIT",
  "authors": [
    {
      "name": "Benedict Wallis",
      "email": "ben@benedict-wallis.com"
    }
  ],
  "require": {
    "php": "^7.3|^8.0",
    "symfony/console": "^4.0|^5.0|^6.0",
    "symfony/process": "^4.2|^5.0|^6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0|^9.3"
  },
  "bin": [
    "bin/pressgang"
  ],
  "autoload": {
    "psr-4": {
      "PressGang\\Installer\\Console\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PressGang\\Installer\\Console\\Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}